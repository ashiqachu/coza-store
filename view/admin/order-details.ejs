    <script>
        document.addEventListener('DOMContentLoaded', function () {
const dropdownButton = document.getElementById('statusDropdown');
const dropdownItems = document.querySelectorAll('.dropdown-item');
const productInfo = document.querySelector('[data-product-id]');
const userInfo = document.querySelector('[data-user-id]')
const orderInfo = document.querySelector('[data-order-id]')

dropdownItems.forEach(function (item) {
item.addEventListener('click', function (event) {
event.preventDefault(); // Prevent the default link behavior
const selectedStatus = event.target.getAttribute('data-status');
dropdownButton.textContent = selectedStatus; // Update the dropdown button text
// You can now use the selectedStatus value as needed
console.log('Selected Status:', selectedStatus);
// alert(selectedStatus)

const orderId = orderInfo.getAttribute('data-order-id')
const productId = productInfo.getAttribute('data-product-id')
const userId =  userInfo.getAttribute('data-user-id')
// alert (userId)
$.ajax({
          method: "POST",
          url: "/admin/statusChanging",
          data: {
            status: selectedStatus,
            id: productId ,// Send the product ID as the 'id' parameter
            userId : userId,
            orderId : orderId
          },
          success : (response) => {
            if (response.status == true) {
              location.reload();
            }
          }
        });
});
});
});

      </script>
      

